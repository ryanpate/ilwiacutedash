        function updateTable(data) {
            const tbody = document.getElementById('tableBody');
            tbody.innerHTML = '';

            const sortingCriteria = document.getElementById('sortingCriteria').value;

            const headers = document.querySelectorAll('#dataTable th');
            headers.forEach(th => {
                th.style.backgroundColor = '#f7fafc';
                th.style.fontWeight = '600';
            });

            const indicator = document.getElementById('sortingIndicator');
            if (sortingCriteria === 'contribution') {
                headers[4].style.backgroundColor = '#e0e7ff';
                headers[4].style.fontWeight = '700';
                indicator.textContent = '(by Contribution - Worst First)';
            } else {
                headers[5].style.backgroundColor = '#e0e7ff';
                headers[5].style.fontWeight = '700';
                indicator.textContent = '(by Total Failures - Highest First)';
            }

            // Calculate total for dynamic severity based on selected criteria
            let datasetTotal = 0;
            if (sortingCriteria === 'contribution') {
                datasetTotal = data.reduce((sum, row) => {
                    const contrib = Number(row.IDXCONTR) || Number(row.AVG_IDXCONTR) || 0;
                    const total = row.TOTAL_IDXCONTR ?
                        Number(row.TOTAL_IDXCONTR) :
                        contrib * (row.RECORD_COUNT || 1);
                    return sum + Math.abs(total);
                }, 0);
            } else {
                datasetTotal = data.reduce((sum, row) => {
                    const total = row.TOTAL_EXTRAFAILURES ?
                        Math.max(0, Number(row.TOTAL_EXTRAFAILURES)) :
                        Math.max(0, Number(row.AVG_EXTRAFAILURES || row.EXTRAFAILURES) || 0) * (row.RECORD_COUNT || 1);
                    return sum + (isFinite(total) ? total : 0);
                }, 0);
            }

            const sortedData = data.slice(0, 100);

            sortedData.forEach((row, index) => {
                const tr = document.createElement('tr');

                const contribution = Number(row.IDXCONTR) || Number(row.AVG_IDXCONTR) || 0;
                const totalContribution = Number(row.TOTAL_IDXCONTR) || (contribution * (row.RECORD_COUNT || 1));

                const avgFailures = Math.max(0, Number(row.AVG_EXTRAFAILURES || row.EXTRAFAILURES) || 0);
                const totalFailures = Math.max(0, Number(row.TOTAL_EXTRAFAILURES) || (avgFailures * (row.RECORD_COUNT || 1)));

                const validAvg = isFinite(avgFailures) ? Math.round(avgFailures) : 0;
                const validTotal = isFinite(totalFailures) ? Math.round(totalFailures) : 0;

                let percentageOfTotal;
                if (sortingCriteria === 'contribution') {
                    percentageOfTotal = datasetTotal !== 0 ?
                        (Math.abs(totalContribution) / datasetTotal) * 100 : 0;
                } else {
                    percentageOfTotal = datasetTotal > 0 ?
                        (validTotal / datasetTotal) * 100 : 0;
                }

                let severity, severityClass;
                if (Math.abs(percentageOfTotal) >= 5) {
                    severity = 'CRITICAL';
                    severityClass = 'severity-high';
                } else if (Math.abs(percentageOfTotal) >= 2.5) {
                    severity = 'HIGH';
                    severityClass = 'severity-high';
                } else if (Math.abs(percentageOfTotal) >= 0.5) {
                    severity = 'MEDIUM';
                    severityClass = 'severity-medium';
                } else {
                    severity = 'LOW';
                    severityClass = 'severity-low';
                }

                // Store severity in row data for filtering
                row.severity = severity;

                const metricDisplay = row.METRIC_DISPLAY || row.METRICNAME || '-';

                const contribColor = contribution < 0 ? '#ef4444' : '#22c55e';
                const contribDisplay = formatContribution(contribution);
                const contribTitle = `Contribution: ${contribution} | Total: ${totalContribution}`;
                const contribCell = `<strong title="${contribTitle}" style="color: ${contribColor}">${contribDisplay}</strong>`;

                const totalFailuresCell = `<strong title="Total: ${formatFullNumber(validTotal)} failures">${formatCompactNumber(validTotal)}</strong>`;

                // Handle district display - now always visible in table
                const district = row.DISTRICT || '-';
                const districtClass = district === '-' ? 'district-badge no-data' : 'district-badge';
                const districtCell = `<td><span class="${districtClass}">${district}</span></td>`;

                tr.innerHTML = `
                    <td><strong>#${index + 1}</strong></td>
                    <td><a href="usid_detail.html?usid=${row.USID}" target="_blank" class="usid-link">${row.USID || '-'}</a></td>
                    ${districtCell}
                    <td>${metricDisplay}</td>
                    <td>${contribCell}</td>
                    <td>${totalFailuresCell}</td>
                    <td>${row.VENDOR || '-'}</td>
                    <td>${row.CQECLUSTER || '-'}</td>
                    <td><span class="${severityClass}" title="${Math.abs(percentageOfTotal).toFixed(2)}% of total">${severity}</span></td>
                `;
                tbody.appendChild(tr);
            });
        }

        function resetFilters() {
            document.getElementById('submarket').value = '';
            
            // Reset district multi-select
            selectedDistricts = [];
            const districtInput = document.getElementById('districtInput');
            districtInput.classList.add('disabled');
            document.getElementById('districtText').textContent = 'Select Submarket First';
            document.getElementById('districtDependentBadge').style.display = 'inline-block';
            availableDistricts = [];
            populateDistrictMultiSelect([]);

            // Reset cascading filter
            currentSubmarketClusters = allClusters;
            document.getElementById('cqeClusterInput').classList.remove('filtered');

            // Reset multi-select clusters
            const checkboxes = document.querySelectorAll('#cqeClusterDropdown input[type="checkbox"]');
            checkboxes.forEach(cb => cb.checked = false);
            selectedClusters = [];

            // Repopulate with all clusters
            populateMultiSelect('cqeCluster', allClusters);
            updateSelectedClusters();

            // Reset district visibility
            updateDistrictVisibility('');
            pendingSubmarket = null;

            document.getElementById('metricName').value = '';
            document.getElementById('usid').value = '';
            document.getElementById('sortingCriteria').value = 'contribution';
            setDefaultDates();

            // Clear column filters and sort
            columnFilters = {};
            currentSort = { column: null, direction: 'asc' };

            fetchData();
        }

        function exportData() {
            if (currentData.length === 0) {
                alert('No data to export');
                return;
            }

            const sortingCriteria = document.getElementById('sortingCriteria').value;
            const isShowingAllMetrics = currentData.length > 0 &&
                (currentData[0].METRIC_DISPLAY === 'All' || currentData[0].METRICNAME === 'ALL');

            const sortedData = currentData;

            const exportData = sortedData.map((row, index) => {
                const contribution = Number(row.IDXCONTR) || Number(row.AVG_IDXCONTR) || 0;
                const totalContribution = Number(row.TOTAL_IDXCONTR) || contribution;
                const avgFailures = Math.max(0, Number(row.AVG_EXTRAFAILURES || row.EXTRAFAILURES) || 0);
                const totalFailures = Math.max(0, Number(row.TOTAL_EXTRAFAILURES) || (avgFailures * (row.RECORD_COUNT || 1)));
                const validAvg = isFinite(avgFailures) ? Math.round(avgFailures) : 0;
                const validTotal = isFinite(totalFailures) ? Math.round(totalFailures) : 0;

                const exportRow = {
                    Rank: index + 1,
                    USID: row.USID || '',
                    District: row.DISTRICT || '-',
                    MetricName: row.METRIC_DISPLAY || row.METRICNAME || '',
                    Contribution: contribution.toFixed(4),
                    TotalContribution: totalContribution.toFixed(4),
                    TotalFailures: validTotal,
                    RecordCount: row.RECORD_COUNT || 1,
                    Vendor: row.VENDOR || '',
                    Cluster: row.CQECLUSTER || '',
                    Submarket: row.SUBMKT || ''
                };

                exportRow.EarliestPeriod = row.EARLIEST_PERIOD || '';
                exportRow.LatestPeriod = row.LATEST_PERIOD || '';
                exportRow.AggregationType = isShowingAllMetrics ? 'All Metrics' : 'Single Metric';
                exportRow.SortedBy = sortingCriteria === 'contribution' ? 'Contribution (Worst First)' : 'Total Failures (Highest First)';
                
                // Add filter information
                if (selectedDistricts.length > 0) {
                    exportRow.FilteredDistricts = selectedDistricts.join(';');
                }

                return exportRow;
            });

            const headers = Object.keys(exportData[0]);
            const csvContent = [
                headers.join(','),
                ...exportData.map(row => headers.map(h => {
                    const value = String(row[h] || '');
                    return value.includes(',') || value.includes('"') || value.includes('\n')
                        ? `"${value.replace(/"/g, '""')}"`
                        : value;
                }).join(','))
            ].join('\n');

            const blob = new Blob([csvContent], { type: 'text/csv' });
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;

            const sortType = sortingCriteria === 'contribution' ? 'contrib_worst_first' : 'failures_highest_first';
            const districtSuffix = selectedDistricts.length > 0 ? `_districts_${selectedDistricts.join('-')}` : '';
            const fileName = isShowingAllMetrics ?
                `cqi_offenders_all_metrics_${sortType}${districtSuffix}_${new Date().toISOString().split('T')[0]}.csv` :
                `cqi_offenders_filtered_${sortType}${districtSuffix}_${new Date().toISOString().split('T')[0]}.csv`;

            a.download = fileName;
            a.click();
            window.URL.revokeObjectURL(url);
        }

        function showLoading() {
            document.querySelector('.loading').style.display = 'block';
            document.querySelector('.table-container').style.display = 'none';
        }

        function hideLoading() {
            document.querySelector('.loading').style.display = 'none';
            document.querySelector('.table-container').style.display = 'block';
        }
    </script>
</body>

</html>