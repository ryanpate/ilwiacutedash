        async function fetchData() {
            const sortingCriteria = document.getElementById('sortingCriteria').value;
            const clusterValues = selectedClusters.length > 0 ? selectedClusters.join(',') : '';
            const districtValues = selectedDistricts.length > 0 ? selectedDistricts.join(',') : '';
            const submarket = document.getElementById('submarket').value;

            // Update district visibility when Apply Filters is clicked
            updateDistrictVisibility(submarket);
            pendingSubmarket = submarket;  

            const filters = {
                submarket: submarket,
                district: districtValues,  // Now sends comma-separated districts
                cqeClusters: clusterValues,
                periodStart: document.getElementById('periodStart').value,
                periodEnd: document.getElementById('periodEnd').value,
                metricName: document.getElementById('metricName').value,
                usid: document.getElementById('usid').value,
                sortingCriteria: sortingCriteria
            };

            console.log('üîÑ Starting fetchData with filters:', filters);
            console.log('üìä Ranking criteria:', sortingCriteria);
            console.log('üìç Selected clusters:', selectedClusters);
            console.log('üìç Selected districts:', selectedDistricts);
            console.log('üìç District data:', submarket ? 'Will be loaded' : 'Not applicable');
            showLoading();

            if (!apiAvailable) {
                console.log('‚ö†Ô∏è API not available, using sample data');
                generateSampleData();
                return;
            }

            try {
                const queryParams = new URLSearchParams(filters).toString();
                const url = `${API_URL}/data?${queryParams}`;
                console.log('üì° Fetching data from:', url);

                const startTime = performance.now();
                const response = await fetch(url, {
                    method: 'GET',
                    headers: {
                        'Content-Type': 'application/json'
                    }
                });
                const endTime = performance.now();

                console.log(`‚è±Ô∏è API response received in ${(endTime - startTime) / 1000}s with status:`, response.status);

                if (response.ok) {
                    console.log('‚úÖ Response OK, parsing JSON...');
                    const responseText = await response.text();
                    console.log(`üìä Response size: ${responseText.length} characters`);

                    try {
                        const data = JSON.parse(responseText);
                        console.log(`‚úÖ JSON parsed successfully. Type: ${typeof data}, Length: ${Array.isArray(data) ? data.length : 'N/A'}`);

                        if (Array.isArray(data)) {
                            console.log(`üìà Data is array with ${data.length} records`);
                            console.log(`üéØ Data is sorted by ${sortingCriteria} at the database level`);

                            // Check if district data is present
                            if (data.length > 0 && data[0].DISTRICT !== undefined) {
                                console.log('üìç District data found in response');
                            }

                            if (data.length > 0) {
                                console.log('üîç First record keys:', Object.keys(data[0]));
                            }
                            currentData = data;
                            updateDashboard(data);
                        } else if (data.error) {
                            console.error('‚ùå API returned error:', data.error);
                            showErrorMessage(`API Error: ${data.error}`);
                            generateSampleData();
                        } else {
                            console.error('‚ùå Unexpected data format:', typeof data);
                            showErrorMessage('Unexpected data format received from API');
                            generateSampleData();
                        }
                    } catch (jsonError) {
                        console.error('‚ùå JSON parsing failed:', jsonError);
                        console.error('Raw response (first 500 chars):', responseText.substring(0, 500));
                        showErrorMessage(`Invalid JSON response: ${jsonError.message}`);
                        generateSampleData();
                    }
                } else {
                    console.error('‚ùå API returned error status:', response.status);
                    const errorText = await response.text();
                    console.error('Error response:', errorText);
                    showErrorMessage(`API Error: ${response.status} - ${errorText}`);
                    generateSampleData();
                }
            } catch (error) {
                console.error('‚ùå Fetch error:', error);
                if (error.name === 'TypeError' && error.message.includes('fetch')) {
                    showErrorMessage('Cannot connect to API - make sure Flask server is running');
                } else if (error.name === 'AbortError' || error.message.includes('timeout')) {
                    showErrorMessage('Request timeout - try a smaller date range');
                } else {
                    showErrorMessage(`Network error: ${error.message}`);
                }
                generateSampleData();
            }
        }

        function generateSampleData() {
            const sampleData = [];
            const metrics = ['V-CDR', 'NS/ESO', 'Quality RSRP', 'Quality RSRQ', 'V-ACC', 'V-ACC-E2E', 'D-ACC', 'DLTPUT', 'ULTPUT', 'D-RET', 'WIFI-RET'];
            const vendors = ['Nokia', 'Ericsson', 'Huawei', 'Samsung'];
            const clusters = ['CQE_EAST', 'CQE_WEST', 'CQE_CENTRAL', 'CQE_SOUTH', 'CQE_NORTH'];
            const submarkets = ['NYC', 'LA', 'Chicago', 'Houston', 'Phoenix', 'Central Illinois'];
            const districts = ['86A', '86B', '86C', '86D', '86E', '86F', '86G', '86H'];

            // Sample submarket-cluster relationships for demo
            submarketClusters = {
                'NYC': ['CQE_EAST', 'CQE_NORTH'],
                'LA': ['CQE_WEST'],
                'Chicago': ['CQE_CENTRAL', 'CQE_NORTH'],
                'Houston': ['CQE_SOUTH', 'CQE_CENTRAL'],
                'Phoenix': ['CQE_WEST', 'CQE_SOUTH'],
                'Central Illinois': ['CQE_CENTRAL']
            };

            const selectedMetric = document.getElementById('metricName').value;
            const selectedSubmarket = document.getElementById('submarket').value;

            // Filter clusters based on selection and submarket
            let availableClusters = clusters;
            if (selectedSubmarket && submarketClusters[selectedSubmarket]) {
                availableClusters = submarketClusters[selectedSubmarket];
            }

            const filteredClusters = selectedClusters.length > 0 ?
                availableClusters.filter(c => selectedClusters.includes(c)) : availableClusters;

            if (!selectedMetric) {
                for (let i = 0; i < 50; i++) {
                    const usid = `${10000 + i}`;
                    
                    // Assign district based on selection or randomly if no selection
                    let assignedDistrict;
                    if (selectedSubmarket) {
                        assignedDistrict = districts[Math.floor(Math.random() * districts.length)];
                        
                        // Skip this record if districts are selected and this doesn't match
                        if (selectedDistricts.length > 0 && !selectedDistricts.includes(assignedDistrict)) {
                            continue;
                        }
                    } else {
                        assignedDistrict = null;
                    }

                    let avgFailures;
                    const rand = Math.random();
                    if (rand > 0.95) {
                        avgFailures = Math.floor(Math.random() * 30000) + 10000;
                    } else if (rand > 0.8) {
                        avgFailures = Math.floor(Math.random() * 5000) + 1000;
                    } else if (rand > 0.5) {
                        avgFailures = Math.floor(Math.random() * 500) + 100;
                    } else {
                        avgFailures = Math.floor(Math.random() * 100);
                    }

                    const recordCount = Math.floor(Math.random() * 100) + 20;
                    const totalFailures = avgFailures * recordCount;

                    const baseContrib = (Math.random() - 0.6) * 0.1;
                    const avgContribution = parseFloat(baseContrib.toFixed(4));
                    const totalContribution = avgContribution * recordCount;

                    sampleData.push({
                        USID: usid,
                        METRICNAME: 'ALL',
                        METRIC_DISPLAY: 'All',
                        AVG_EXTRAFAILURES: avgFailures,
                        TOTAL_EXTRAFAILURES: totalFailures,
                        AVG_IDXCONTR: avgContribution,
                        TOTAL_IDXCONTR: totalContribution,
                        IDXCONTR: avgContribution,
                        RECORD_COUNT: recordCount,
                        EXTRAFAILURES: avgFailures,
                        VENDOR: vendors[Math.floor(Math.random() * vendors.length)],
                        CQECLUSTER: filteredClusters[Math.floor(Math.random() * filteredClusters.length)],
                        SUBMKT: selectedSubmarket || submarkets[Math.floor(Math.random() * submarkets.length)],
                        DISTRICT: assignedDistrict,
                        EARLIEST_PERIOD: new Date(Date.now() - recordCount * 24 * 60 * 60 * 1000).toISOString(),
                        LATEST_PERIOD: new Date().toISOString()
                    });
                }
            }

            // Update filter dropdowns
            allClusters = clusters;
            currentSubmarketClusters = availableClusters;

            populateSelect('submarket', submarkets);
            populateMultiSelect('cqeCluster', availableClusters);
            populateSelect('metricName', metrics);

            currentData = sampleData;
            updateDashboard(sampleData);
